{
  "name": "@fernandoherlo/vue-core",
  "version": "0.10.17",
  "author": "Fernando Herlo",
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandoherlo/vue-core.git"
  },
  "bugs": {
    "url": "https://github.com/fernandoherlo/vue-core/issues"
  },
  "homepage": "https://github.com/fernandoherlo/vue-core#readme",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve 'demo/main'",
    "build": "vue-cli-service build --target lib --name vue-core src/build.js",
    "lint": "vue-cli-service lint",
    "repo_a": "git add .",
    "repo_u": "git add -u",
    "repo_c": "git commit -m 'Bugs'",
    "repo": "npm run repo_a && npm run repo_u && npm run repo_c",
    "ver": "npm version patch",
    "push": "git push origin master ",
    "prod": "npm run build && npm run repo && npm run ver && npm run push && npm publish --access=public"
  },
  "main": "./dist/vue-core.common.js",
  "dependencies": {
    "auth0-js": "^9.11.2",
    "axios": "^0.18.1",
    "bootstrap-vue": "^2.0.0-rc.27",
    "vee-validate": "^2.2.13",
    "vue": "^2.6.10",
    "vue-awesome": "^3.5.4",
    "vue-good-table": "^2.17.3",
    "vue-notification": "^1.3.16",
    "vue-router": "^3.0.7",
    "vuejs-datepicker": "^1.6.2",
    "vuejs-logger": "1.5.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.3",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  }
}
